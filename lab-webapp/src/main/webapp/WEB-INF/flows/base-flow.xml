<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://www.springframework.org/schema/webflow http://www.springframework.org/schema/webflow/spring-webflow-2.4.xsd" 
      start-state="setUp" abstract="true">
      
        <action-state id="setUp" >
                <evaluate expression="formAction.setUp" />
                <transition on="success" to="initial"/>
        </action-state>
    
        <global-transitions>
                <transition on="cancel" to="final" />
                <transition on="error" to="error" />
        </global-transitions>
	 
        <end-state id="error" view="error" />
         
        <bean-import resource="base-beans.xml"/>
    
</flow>